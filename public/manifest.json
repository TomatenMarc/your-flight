{
  "manifest_version": 3,
  "name": "Your Flight",
  "version": "0.1",
  "description": "",
  "background": {
    "service_worker": "./background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "permissions": [],
  "author": "Marc Feger and Jan Steimann",
  "homepage_url": "https://github.com/TomatenMarc/your-flight",
  "short_name": "Your Flight",
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Chrome Extension",
    "default_icon": {
      "16": "/images/flight",
      "32": "/images/flight.png",
      "48": "/images/flight.png",
      "128": "/images/flight.png"
    }
  },
  "icons": {
    "16": "/images/flight.png",
    "32": "/images/flight.png",
    "48": "/images/flight.png",
    "128": "/images/flight.png"
  }
}
